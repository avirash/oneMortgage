       "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome12",
       "logic": "input[fixedTermContract1] + input[renewableContractPAYE1] + input[drawdownOnPensionFund1] + input[pensionsAndAnnuities1] + input[statePension1] + input[employedInFfamilyBusiness1]",
