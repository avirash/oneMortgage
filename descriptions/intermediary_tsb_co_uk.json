     },
     "fixedTermContract2": {
       "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome22",
       "logic": "input[fixedTermContract2] + input[renewableContractPAYE2] + input[drawdownOnPensionFund2] + input[pensionsAndAnnuities2] + input[statePension2] + input[employedInFfamilyBusiness2]"
     }
   }
   },
   "earnAnyEmployedAllowances": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome3",
     "logic": "('@value' === 'Yes')  ? 'Other Income' : 'none' ",
     "Other Income": {
       "carAllowance1": {
         "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome13",
         "logic": "input[carAllowance1] + input[weightingLargeTown1] + input[permanentShiftAllowance1] + input[indefiniteMortgageSubsidies1] + input[long-termMortgageSubsidies1]"
       },
       "carAllowance2": {
         "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome23",
         "logic": "input[carAllowance2] + input[weightingLargeTown2] + input[permanentShiftAllowance2] + input[indefiniteMortgageSubsidies2] + input[long-termMortgageSubsidies2]"
       }
     }
   },
   "receiveAnyGovernmentBenefits": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome4",
     "logic": "('@value' === 'Yes')  ? 'Other Income' : 'none' ",
     "Other Income": {
       "childBenefit1": {
         "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome14",
         "logic": "input[childBenefit1] + input[childTaxCredits1] + input[workingTaxCredits1] + input[indefiniteStateBenefits1]"
       },
       "childBenefit2": {
         "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_otherincome24",
         "logic": "input[childBenefit2] + input[childTaxCredits2] + input[workingTaxCredits2] + input[indefiniteStateBenefits2]"
       }
     }
   },
   "monthlyHirePurchasePayments": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_credit1",
     "logic": "input[totalCreditCards] + input[monthlyHirePurchasePayments] + input[monthlyLoanPayments]"
   },
   "creditCardBalances": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_card1"
   },
   "otherCommittedPayments": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_additionalcosts1",
     "logic": "input[otherCommittedPayments] + input[childcareAndEducation] + input[healthInsurancePayments] + input[privatePensions] + input[buildingContentsInsurance] + input[groundRent] + input[serviceCharge] + input[additionalCommittedExpenditure]"
   },
   "otherMortgagePayments": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_txtMonthlyPaymentCompletion"
   },
   "numOfApplying": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_adults"
   },
   "numOfdependants": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_children",
     "logic": "parseInt(input[childUnder5]) + parseInt(input[childUnder11]) + parseInt(input[childUnder17]) + parseInt(input[childOver18])",
     "new": true
   },
   "mortgageTermYears": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_term"
   },
   "loanValue": {
     "css": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_txtLTV",
     "logic": "((input[loanValue] / input[propertyValue]) * 100).toFixed(2)"
   },
   "click1": "#ctl00_mainContentPlaceHolder_dzMain_uxColumnDisplay_ctl00_uxControlColumn_ctl01_uxWidgetHost_uxWidgetHost_widget_ctl00_submitproduct"
  },
  "extraction": {
